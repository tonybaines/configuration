<Eventing>
    <processName>broker</processName>
    <stack>
        <nodeName>YOUR_NODE_ID</nodeName>
        <clusterName>YOUR_CLUSTER_NAME</clusterName>
        <clusterNodes>OTHER_NODE_ADDRESSES</clusterNodes>
        <enterpriseName>YOUR_ENTERPRISE_NAME</enterpriseName>
    </stack>
    <broker>
        <executors>
            <schedulerExecutor>
                <corePoolSize>4</corePoolSize>
            </schedulerExecutor>
            <stripedExecutor>
                <workerThreads>32</workerThreads>
                <workersQueueSize>1000000</workersQueueSize>
            </stripedExecutor>
            <newExecutor>
                <coreThreads>4</coreThreads>
                <maxThreads>16</maxThreads>
                <queueSize>4096</queueSize>
            </newExecutor>
        </executors>
        <directChannelHousekeeper>
            <messageTimeToLiveMillis>900000</messageTimeToLiveMillis>
            <checkPeriodMillis>60000</checkPeriodMillis>
        </directChannelHousekeeper>
    </broker>
    <frontend>
        <webSocket>
            <timeout>604800000</timeout>
            <maxTextFrameSize>8192</maxTextFrameSize>
        </webSocket>
        <http>
            <port>8080</port>
            <securePort>9443</securePort>
            <bypassVip>false</bypassVip>
            <vip>YOUR_CLUSTER_VIP</vip>
        </http>
        <stomp>
            <minHeartbeatInterval>30000</minHeartbeatInterval>
        </stomp>
        <security>
            <enabled>false</enabled>
            <basicAuthDelegateName>PASS_THROUGH</basicAuthDelegateName>
        </security>
        <transparentProxy>
            <timeout>2400000</timeout>
            <idleTimeout>2400000</idleTimeout>
            <toUri>http://YOUR_NODE_IP:7080</toUri>
        </transparentProxy>
    </frontend>
</Eventing>
